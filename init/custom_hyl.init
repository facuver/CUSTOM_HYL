#!/bin/sh


EARLY=/usr/local/init/custom_hyl_early

echo +++ $EARLY start
$EARLY
echo +++ $EARLY complete


(
	while [ ! -e /var/www/d-tacq/rc-local-complete ]; do
		sleep 1
	done
	ONBOOT=/usr/local/init/custom_hyl_onboot
	echo +++ $ONBOOT start
	$ONBOOT
	echo +++ $ONBOOT complete
	date > /var/www/d-tacq/custom_hyl_onboot_complete
) &
